
@{
    ViewBag.Title = "UserNotFound";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="panel panel-danger col-md-10 form-margin-top">
        <div class="panel-heading">User Not Found</div>
        <div class="panel-body">
            <div class="container">
                <div class="row">
                    <p>The User cannot be Found</p><br /><p>Please Log in or Register to be a user using the links below</p>
                </div>
                <div class="row">
                    
                    <a href="/Account/Login" class="btn btn-default">Login</a>
                    <a href="/Account/Register" class="btn btn-default">Register</a>
                </div>

            </div>
        </div>

    </div>
</div>